{
  "name": "progressivis-nb-widgets",
  "version": "0.1.0",
  "description": "A Custom Jupyter Widget Library for Progressivis",
  "author": "Jean-Daniel Fekete",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdfekete/progressivis/progressivis_nb_widgets"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "css/**/*.css",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "prepublish": "npm run build:babel",
    "build:babel": "rm -rf lib/*.js && babel src/*.js* --out-dir lib",
    "build": "npm run build:babel && webpack",
    "watch": "webpack --watch --mode=development",
    "eslint": "eslint src",
    "jshint": "jshint src/*.js",
    "prettier": "prettier --write src/*.js*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/compat-data": "^7.9.0",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.2",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3",
    "chroma-js": "",
    "core-js": "^3.6.4",
    "d3": "5.0.0",
    "datatables": "^1.10.18",
    "jquery": "^3.4.1",
    "jquery-sparkline": "^2.4.0",
    "jupyter-dataserializers": "^2.2.0",
    "jupyter-datawidgets": "^5.3.0",
    "lodash": "^4.17.4",
    "multiclass-density-maps": "~0.1.5",
    "ndarray": "^1.0.18",
    "ndarray-unpack": "^1.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "sorttable": "^1.0.2",
    "webcola": "^3.4.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  },
  "prettier": {
    "singleQuote": true
  },
  "jshintConfig": {
    "esversion": 6,
    "node": true,
    "laxbreak": true,
    "varstmt": true,
    "undef": true,
    "unused": true,
    "globals": {
      "console": false,
      "document": false,
      "module": true,
      "window": false,
      "MutationObserver": false,
      "__webpack_public_path__": true,
      "sorttable": false
    }
  }
}
